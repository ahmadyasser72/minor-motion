{
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "overrides": [
    { "files": "*.astro", "options": { "parser": "astro" } },
    { "files": "*.svelte", "options": { "parser": "svelte" } }
  ],
  "importOrder": [
    "^[.]",
    "",
    "^\\$lib",
    "",
    "^svelte\\/",
    "^astro:?",
    "",
    "<THIRD_PARTY_MODULES>"
  ],
  "endOfLine": "crlf",
  "semi": true,
  "trailingComma": "es5"
}
